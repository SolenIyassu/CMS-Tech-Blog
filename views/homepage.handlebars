{{#each posts from |post|}}
<div class="row mb-4 post">
<div class="col-md-5">
<h2>
  {{!-- {{{get_emoji}}} --}}
  <a href="/post/{{projec.id}}">{{project.name}}</a>
</h2>
<p>
  <span for="img" aria-label="post"></span>
</p>
<p>Created by: {{post.user.name}} on {{format_date post.date_created}}</p>
</div>
<div class="col-md-7">
  <p>
    {{post.description}}
  </p>
</div>
</div>
{{each}}